    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Python File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}"
        },
        {
            "name": "Python: Debug with vocadev",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "env": {
                "PYTHONPATH": "/home/krisspy/miniconda3/envs/vocadev/bin/python3"
            },
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/.env"
        }
    ]
}